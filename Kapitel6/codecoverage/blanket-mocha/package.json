{
  "name": "blanket-mocha",
  "version": "1.0.0",
  "description": "Blanket.js Mocha Beispiel",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "blanket": "^1.1.6"
  },
  "devDependencies": {},
  "scripts": {
    "test": "mocha"
  },
